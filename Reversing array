#include <iostream>
using namespace std;
int main()
{
    int A[5] = {1, 2, 3, 4, 5};
    int N = 5;
    int j = 0;
    int B[5];

    for (int i = 0; i < N; i++)
    {
        B[j] = A[i];
        j++;
    }

    j = 4;

    for (int i = 0; i < N; i++)
    {
        A[i] = B[j];
        j--;
    }

    for (int i = 0; i < N; i++)
    {
        cout << A[i];
    }
}
